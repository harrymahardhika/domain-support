{
    "name": "harrym/domain-support",
    "description": "Laravel Domain Support",
    "license": "MIT",
    "authors": [
        {
            "name": "Harry Mahardhika",
            "email": "harry@infinitidea.com",
            "homepage": "https://harrymahardhika.com"
        }
    ],
    "homepage": "https://github.com/harrym/domain-support",
    "keywords": [
        "Laravel",
        "DomainSupport"
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-data": "^4.15"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^12.5",
        "orchestra/testbench": "^10.8",
        "pestphp/pest": "^4.2",
        "laravel/pint": "^1.18",
        "larastan/larastan": "^3.8",
        "rector/rector": "^2.2",
        "driftingly/rector-laravel": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "HarryM\\DomainSupport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HarryM\\DomainSupport\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HarryM\\DomainSupport\\DomainSupportServiceProvider"
            ],
            "aliases": {
                "DomainSupport": "HarryM\\DomainSupport\\Facades\\DomainSupport"
            }
        },
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
